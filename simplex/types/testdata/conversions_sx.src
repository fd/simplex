package conversions

func _table_to_keyed_view() {
  var t table[string]float64
  var v1 view[int]float64
  var v2 view[string]int
  var v3 view[string]float64

  t = v3 /* ERROR "cannot convert view into table" */

  v1 = t /* ERROR "wrong key type" */
  v2 = t /* ERROR "wrong value type" */
  v3 = t
}

func _table_to_indexed_view() {
  var t table[string]float64
  var v1 view[]int
  var v2 view[]float64

  t = v2 /* ERROR "cannot convert view into table" */

  v1 = t /* ERROR "wrong value type" */
  v2 = t
}

func _keyed_view_to_indexed_view() {
  var k view[string]float64
  var v1 view[]int
  var v2 view[]float64

  k = v2 /* ERROR "cannot convert indexed view into keyed view" */

  v1 = k /* ERROR "wrong value type" */
  v2 = k
}
